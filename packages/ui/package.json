{
  "name": "@student-helper/ui",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./globals.css": "./src/web/styles/globals.css",
    "./postcss.config": "./postcss.config.mjs",
    "./utils/*": "./src/utils/*.ts",
    "./tokens": "./src/tokens/index.ts",
    "./tokens/*": "./src/tokens/*.ts",
    "./types": "./src/types/index.ts",
    "./types/*": "./src/types/*.ts",
    "./web": "./src/web/index.ts",
    "./web/primitives/*": "./src/web/primitives/*.tsx",
    "./web/components/*": "./src/web/components/*.tsx",
    "./web/hooks/*": "./src/web/hooks/*.ts",
    "./lib/*": "./src/utils/*.ts"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.564.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@student-helper/eslint": "*",
    "@student-helper/tsconfig": "*",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.2"
  }
}
